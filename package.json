{
  "name": "Agility",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql": "^14.6.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "app-root-path": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "cuid": "^2.1.8",
    "custom-env": "^2.0.1",
    "easygraphql-tester": "^5.1.6",
    "ejs": "^3.0.1",
    "eslint-plugin-graphql": "^3.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql-compose": "^7.11.0",
    "graphql-compose-mongoose": "^7.3.4",
    "graphql-iso-date": "^3.6.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.10",
    "mongoose": "^5.9.1",
    "nodemailer-promise": "^2.0.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "restoreMocks": true,
    "testPathIgnorePatterns": [
      "dist/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!src/index.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/utils/testconfig.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "lint": "eslint --ext .js ./src",
    "build": "babel src --out-dir dist",
    "test": "jest --coverage",
    "test:watch:coverage": "jest --coverage --watchAll",
    "test:watch": "jest --watchAll",
    "pretest": "npm run test:clean && npm run lint:eslint:fix",
    "lint:staged": "lint-staged",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
    "test:clean": "rimraf ./coverage"
  }
}
